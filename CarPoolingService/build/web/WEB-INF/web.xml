<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>CarServlet</servlet-name>
        <servlet-class>com.Project1.CarPoolingService.Servlet.CarServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RouteServlet</servlet-name>
        <servlet-class>com.Project1.CarPoolingService.Servlet.RouteServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SignUpServlet</servlet-name>
        <servlet-class>com.Project1.CarPoolingService.Servlet.SignUpServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SingInServlet</servlet-name>
        <servlet-class>com.Project1.CarPoolingService.Servlet.SingInServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>carbookinglistServlet</servlet-name>
        <servlet-class>com.Project1.CarPoolingService.Servlet.carbookinglistServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>carpoolbookinglistServlet</servlet-name>
        <servlet-class>com.Project1.CarPoolingService.Servlet.carpoolbookinglistServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CarServlet</servlet-name>
        <url-pattern>/Car.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RouteServlet</servlet-name>
        <url-pattern>/Route.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SignUpServlet</servlet-name>
        <url-pattern>/SignUp.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SingInServlet</servlet-name>
        <url-pattern>/SingIn.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>carbookinglistServlet</servlet-name>
        <url-pattern>/carbookinglist.view</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>carpoolbookinglistServlet</servlet-name>
        <url-pattern>/carpoolbookinglist.view</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
